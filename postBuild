#!/bin/bash

wget https://github.com/quarto-dev/quarto-cli/releases/download/v1.2.313/quarto-1.2.313-linux-amd64.deb
apt install quarto-1.2.313-linux-amd64.deb

Rscript setup_cran.R

Rscript setup_gh.R

rm apt.txt README.md environment.yml postBuild setup_cran.R setup_gh.R quarto-1.2.313-linux-amd64.deb
